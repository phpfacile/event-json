{
    "name": "phpfacile/event-json",
    "description": "This service basically perform a mapping between an event retrieved from a database as an array to an event as an array that will be easily transformed (using json_encode()) in a target JSON string formatted as expected.",
    "keywords": ["event", "json"],
    "homepage": "https://github.com/phpfacile/event-json",
    "license": "MIT",
    "authors": [
        {
            "name": "PHP Facile!",
            "homepage": "https://www.phpfacile.com/"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*"
    },
    "scripts": {
        "cs-check": "phpcs -s src",
        "cs-check-psr": "phpcs -s --standard=PSR1,PSR2 src",
        "cs-check-pear": "phpcs -s --standard=PEAR src",
        "cs-fix": "phpcbf src"
    },
    "autoload": {
        "psr-4": {
          "PHPFacile\\Event\\Json\\": "src/"
        }
    }
}
